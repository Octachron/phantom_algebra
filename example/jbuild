(jbuild_version 1)

(executable (
  (name a)
  (modules (A))
  (libraries (phantom_algebra))
  (preprocess (action (run ${bin:cppo} -V OCAML:${ocaml_version} ${<})))
))



(executable (
            (name bug)
            (modules (Bug))
            (libraries (phantom_algebra))
))


(alias
 ((name    runtest)
  (deps    (a.exe))
  (action  (run ${<}))
))
